    "node": ">=8.1.0"
  },
  "description": "Pubsub integration into atrix microservice framework",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --timeout 30000 --trace-warnings $(find . -name '*.specs.js' -not -path './node_modules/*')",
