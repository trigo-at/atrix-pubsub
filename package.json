    "avsc": "^5.0.5",
    "bluebird": "^3.5.3",
    "ioredis": "^3.1.4",
    "joi": "^14.3.0",
    "mqemitter": "^2.2.0",
    "mqemitter-redis": "^2.2.0",
    "promise-retry": "^1.1.1",
    "ramda": "^0.26.1",
    "shot": "^4.0.7",
    "uuid": "^3.0.1"
